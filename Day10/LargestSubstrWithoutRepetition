class Solution {
    public int lengthOfLongestSubstring(String s) {
        int right=0;
        int left=0;
        int max_len=0;

        HashSet<Character> hs=new HashSet<Character>();

        for(right=0;right<s.length();right++)
        {
            char c=s.charAt(right);
            while(hs.contains(c))
            {
                hs.remove(s.charAt(left));
                left++;
            }
            hs.add(c);
            max_len=Math.max(max_len,(right-left+1));
        }

        return max_len;
    }
}
